<groups xmlns="http://www.isdc.ro/wro">
	<group name="wd">
		<js>/js/App.coffee</js>
		<js>/js/Constants.coffee</js>
		<js minimize="false">/js/controllers/**.coffee</js>
		<js minimize="false">/js/services/**.coffee</js>
		<js>/js/util/wd-util.coffee</js>

		<css>/css/wd-reset.less</css>
		<css>/css/wd-desktop.less</css>
	</group>

	<group name="wd-vendor">
		<js minimize="false">webjar:modernizr.min.js</js>
