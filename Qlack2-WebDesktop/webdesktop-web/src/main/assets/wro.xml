		<js minimize="false">webjar:angular.min.js</js>
		<js minimize="false">/js-vendor/angular-cookies.min.js</js>
		<js minimize="false">/js-vendor/kendo.web.min.js</js>
		<js>/js-vendor/angular-kendo.js</js>
		<js minimize="false">webjar:angular-ui-router.min.js</js>
		<js minimize="false">webjar:SecuritySrvProvider.js</js>
		<js>/js-vendor/jquery.clock.js</js>
		<js>/js-vendor/jquery.contextmenu.js</js>
		<js minimize="false">webjar:jquery.easing.min.js</js>
		<js>/js-vendor/jquery.notifications.js</js>
		<js>/js-vendor/jquery.md5.js</js>
		<js minimize="false">webjar:soundmanager2-jsmin.js</js>
		<js minimize="false">/js-vendor/jquery.qtip.min.js</js>
		<js>webjar:jquery.form.js</js>
		<js>webjar:cometd.js</js>
		<js>webjar:TimeSyncExtension.js</js>
		<js>webjar:TimeStampExtension.js</js>
		<js>webjar:AckExtension.js</js>
